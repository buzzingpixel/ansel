{
    "name": "buzzingpixel/ansel",
    "version": "3.0.0",
    "type": "craft-plugin",
    "description": "A field type for defining image requirements and non-destructive editing of images per-field.",
    "license": "proprietary",
    "homepage": "https://www.buzzingpixel.com",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://www.buzzingpixel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Ansel\\": "src/",
            "BuzzingPixel\\AnselConfig\\": "config/",
            "BuzzingPixel\\AnselCms\\": "cms/",
            "BuzzingPixel\\AssetsDist\\": "assetsDist/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BuzzingPixel\\Treasury\\": "devResources/treasury-ee/treasury"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "extra": {
        "changelogUrl": "https://github.com/buzzingpixel/ansel-craft/blob/master/changelog.md",
        "class": "BuzzingPixel\\AnselCms\\Craft\\AnselCraftPlugin",
        "developer": "BuzzingPixel, LLC",
        "developerUrl": "https://www.buzzingpixel.com",
        "documentationUrl": "https://www.buzzingpixel.com/software/ansel-craft/documentation",
        "handle": "ansel",
        "hasCpSettings": true,
        "name": "Ansel",
        "schemaVersion": "3.0.0",
        "systemPath": "cms/ExpressionEngine",
        "themePath": "assetsDist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "expression-engine": {
            "type": "package",
            "package": {
                "name": "expressionengine/expressionengine",
                "version": "6.dev",
                "source": {
                    "url": "https://github.com/ExpressionEngine/ExpressionEngine.git",
                    "type": "git",
                    "reference": "6.dev"
                }
            }
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "buzzingpixel/container": "^1.1",
        "buzzingpixel/twig-switch": "^1.0",
        "cebe/markdown": "^1.2",
        "cocur/slugify": "^4.1",
        "guzzlehttp/guzzle": "^6.5.5|^7.2.0",
        "league/mime-type-detection": "^1.9",
        "marc1706/fast-image-size": "^1.1",
        "ramsey/uuid": "^4.2",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "twig/twig": "^2"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "craftcms/cms": "^3.7",
        "doctrine/coding-standard": "^9",
        "expressionengine/expressionengine": "*",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^5.3"
    }
}
